#include <iostream>
using namespace std;

class Stack {
    char arr[100];
    int top;
public:
    Stack() { top = -1; }
    
    void push(char c) {
        arr[++top] = c;
    }
    
    char pop() {
        return arr[top--];
    }
    
    char getTop() {
        return arr[top];
    }
    
    bool isEmpty() {
        return top == -1;
    }
};

bool isOperand(char c) {
    return (c >= 'a' && c <= 'z') || (c >= 'A' && c <= 'Z') || (c >= '0' && c <= '9');
}

int getPrecedence(char op) {
    if (op == '^')
        return 3;
    else if (op == '*' || op == '/')
        return 2;
    else if (op == '+' || op == '-')
        return 1;
    else
        return 0;
}

void reverse(char str[]) {
    int len = 0;
    while (str[len] != '\0') len++;
    
    for (int i = 0; i < len/2; i++) {
        char temp = str[i];
        str[i] = str[len-1-i];
        str[len-1-i] = temp;
    }
}

void infixToPostfix(char infix[], char postfix[]) {
    Stack s;
    int j = 0;
    
    for (int i = 0; infix[i] != '\0'; i++) {
        char c = infix[i];
        
        if (isOperand(c)) {
            postfix[j++] = c;
        }
        else if (c == '(') {
            s.push(c);
        }
        else if (c == ')') {
            while (!s.isEmpty() && s.getTop() != '(') {
                postfix[j++] = s.pop();
            }
            if (!s.isEmpty())
                s.pop();
        }
        else {
            while (!s.isEmpty() && getPrecedence(s.getTop()) >= getPrecedence(c)) {
                postfix[j++] = s.pop();
            }
            s.push(c);
        }
    }
    
    while (!s.isEmpty()) {
        postfix[j++] = s.pop();
    }
    
    postfix[j] = '\0';
}

void infixToPrefix(char infix[], char prefix[]) {
    reverse(infix);
    
    for (int i = 0; infix[i] != '\0'; i++) {
        if (infix[i] == '(')
            infix[i] = ')';
        else if (infix[i] == ')')
            infix[i] = '(';
    }
    
    infixToPostfix(infix, prefix);
    reverse(prefix);
}

int main() {
    char infix[100], postfix[100], prefix[100], original[100];
    
    cout << "Enter infix expression: ";
    cin >> infix;
    
    int i = 0;
    while (infix[i] != '\0') {
        original[i] = infix[i];
        i++;
    }
    original[i] = '\0';
    
    infixToPostfix(original, postfix);
    infixToPrefix(infix, prefix);
    
    cout << "Postfix: " << postfix << endl;
    cout << "Prefix: " << prefix << endl;
    
    return 0;
}




